#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/esprima@4.0.1/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/esprima@4.0.1/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../bin/esvalidate.js" "$@"
fi
