{
  "name": "@mikro-orm/sql-highlighter",
  "version": "1.0.1",
  "description": "Node.js SQL highlighter for CLI based on doctrine/sql-formatter.",
  "main": "index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mikro-orm/sql-highlighter.git"
  },
  "keywords": [
    "sql",
    "nodejs",
    "highlight",
    "tokenizer"
  ],
  "author": "Martin AdÃ¡mek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikro-orm/sql-highlighter/issues"
  },
  "homepage": "https://mikro-orm.io",
  "engines": {
    "node": ">= 10.13.0"
  },
  "scripts": {
    "release": "yarn build && cd dist && npm publish && cd ..",
    "build": "yarn clean && yarn compile && yarn copy",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.json",
    "copy": "ts-node -T ./scripts/copy.ts",
    "lint": "eslint src/*.ts",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ],
    "modulePathIgnorePatterns": [
      "package.json",
      "<rootDir>/package.json"
    ]
  },
  "dependencies": {
    "ansi-colors": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "~3.5.0",
    "@typescript-eslint/parser": "~3.5.0",
    "eslint": "^7.6.0",
    "jest": "^26.2.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
