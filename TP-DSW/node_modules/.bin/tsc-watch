else
  export NODE_PATH="/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.1.3/node_modules/tsc-watch/dist/lib/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.1.3/node_modules/tsc-watch/dist/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.1.3/node_modules/tsc-watch/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/tsc-watch@6.0.4_typescript@5.1.3/node_modules:/mnt/c/Users/maste/Desktop/AAAAAAAAAAA/TP-DSW---COM-302---Jeandrevin-Conti-Ivanisky-De-Luca/TP-DSW/node_modules/.pnpm/node_modules:$NODE_PATH"
