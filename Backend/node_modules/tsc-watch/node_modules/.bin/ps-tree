#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/Desktop/tp-dsw/Backend/node_modules/.pnpm/ps-tree@1.2.0/node_modules/ps-tree/bin/node_modules:/mnt/c/Users/Usuario/Desktop/tp-dsw/Backend/node_modules/.pnpm/ps-tree@1.2.0/node_modules/ps-tree/node_modules:/mnt/c/Users/Usuario/Desktop/tp-dsw/Backend/node_modules/.pnpm/ps-tree@1.2.0/node_modules:/mnt/c/Users/Usuario/Desktop/tp-dsw/Backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/Desktop/tp-dsw/Backend/node_modules/.pnpm/ps-tree@1.2.0/node_modules/ps-tree/bin/node_modules:/mnt/c/Users/Usuario/Desktop/tp-dsw/Backend/node_modules/.pnpm/ps-tree@1.2.0/node_modules/ps-tree/node_modules:/mnt/c/Users/Usuario/Desktop/tp-dsw/Backend/node_modules/.pnpm/ps-tree@1.2.0/node_modules:/mnt/c/Users/Usuario/Desktop/tp-dsw/Backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ps-tree@1.2.0/node_modules/ps-tree/bin/ps-tree.js" "$@"
else
  exec node  "$basedir/../../../../../ps-tree@1.2.0/node_modules/ps-tree/bin/ps-tree.js" "$@"
fi
